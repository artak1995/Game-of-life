{
  "name": "game-of-life-server",
  "version": "1.1.0",
  "main": "app.js",
  "author": "billy",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "yarn clean && yarn build-babel && yarn build-react",
    "build-babel": "babel -d ./build ./src -s",
    "build-react": "cd client && yarn && yarn build",
    "start": "yarn build && node ./build/app.js",
    "dev": "nodemon --exec babel-node src/app.js",
    "heroku-postbuild": "yarn build-react"
  },
  "dependencies": {
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "nodemon": "^1.19.2"
  }
}